<%= error_messages_for :audio_file %>

<h1>New audio file</h1>

<% form_for(:audio_file, :url => audio_files_path, :method => :put,
            :html => { :multipart => true }) do |f| -%>


  <%= render :partial => 'form', :locals => { :f => f } -%>

  <p>
    <%= submit_tag 'Create' %>
  </p>

  <br />
<% end -%>
